  {% include 'clients_dashboard/Components/Scripts.html.twig' %}
  <script src="{{ asset('Clients/Assets/js/pages/widgets.js') }}"></script>
  <script src="{{
    asset(
      'Clients/Assets/js/pages/custom/profile/profile.js'
    )
    }}"></script>
{% endblock %}
