      <!-- begin::Content -->
      <div class="flex-row-fluid ml-lg-8">
        <!-- begin::Card -->
        <div class="card card-custom card-stretch">
          <!-- begin::Header -->
          <div class="card-header py-3">
            <div class="card-title align-items-start flex-column">
              <h3 class="card-label font-weight-bolder text-dark">
                Informations de L'Utilisateur
              </h3>
              <span class="text-muted font-weight-bold font-size-sm mt-1">
                Mettre a jour votre Profil
              </span>
            </div>
          <!-- begin::Form -->
          {{ form_start(form,{'attr':{'class':'form'}}) }}
            <div class="card-toolbar">
              <button type="submit" class="btn btn-success mr-2">
                Sauvegarder Les Modifications
              </button>
              <a href={{ path('ClientsDashboard') }} type="reset" class="btn btn-secondary">Annuler</a>
            </div>
          </div>
          <!-- end::Header -->

            <!-- begin::Body -->
            <div class="card-body">
              <div class="row">
                <label class="col-xl-3"></label>
                <div class="col-lg-9 col-xl-6">
                  <h5 class="font-weight-bold mb-6">
                    Vos Informations Profil
                  </h5>
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">Photo De Profil</label>
                <div class="col-lg-9 col-xl-6">
                  <div class="image-input image-input-outline"
                    id="kt_profile_avatar"
                    style="background-image: url({{
                        asset('Admins/Assets/media/users/blank.png')}})">
                    {% if client.image != null %}
                      <div class="image-input-wrapper"
                        style="background-image: url({{
                        asset('Clients/PhotosDeProfil/')
                          ~ client.image
                        }})"
                        alt="votre Photo de Profil"></div>
                    {% else %}
                      <div class="image-input-wrapper"
                        style="background-image: url({{
                        asset(
                          'Admins/Assets/media/users/blank.png'
                        )
                        }})"
                        alt="Vous n'avez pas de Photo de Profil"></div>
                    {% endif %}
                    <label class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow"
                      data-action="change"
                      data-toggle="tooltip"
                      title=""
                      data-original-title="Changer l'avatar">
                      <i class="fa fa-pen icon-sm text-muted"></i>
                      {{ form_widget(form.Image,{'attr':{'class':'form-control form-control-solid form-control-lg','accept':'.png,.jpg,.jpeg'}}) }}
                    </label>

                    <span class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow"
                      data-action="cancel"
                      data-toggle="tooltip"
                      title=""
                      data-original-title="Supprimer avatar">
                      <i class="ki ki-bold-close icon-xs text-muted"></i>
                    </span>

                    <span class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow"
                      data-action="remove"
                      data-toggle="tooltip"
                      title=""
                      data-original-title="Enlever l'avatar">
                      <i class="ki ki-bold-close icon-xs text-muted"></i>
                    </span>
                  </div>
                  <span class="form-text text-muted">
                    Fichiers Permis: png, jpg, jpeg.
                  </span>
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Nom
                </label>
                <div class="col-lg-9 col-xl-6">
                  {{ form_widget(form.Nom,{'attr':{'class':'form-control form-control-lg form-control-solid','name':'nom'}}) }}
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Prenom
                </label>
                <div class="col-lg-9 col-xl-6">
                  {{ form_widget(form.Prenom,{'attr':{'class':'form-control form-control-lg form-control-solid','name':'prenom'}}) }}
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Age
                </label>
                <div class="col-lg-9 col-xl-6">
                  {{ form_widget(form.Age,{'attr':{'class':'form-control form-control-lg form-control-solid','name':'age'}}) }}
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Sexe
                </label>
                <div class="col-lg-9 col-xl-6">
                  {{ form_widget(form.Sexe,{'attr':{'class':'form-control form-control-lg form-control-solid','name':'sexe'}}) }}
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Address
                </label>
                <div class="col-lg-9 col-xl-6">
                    {{ form_widget(form.Adresse,{'attr':{'class':'form-control form-control-lg form-control-solid'}}) }}
                    <span class="form-text text-muted">
                      Au cas ou vous ne pouvez pas renseigner une Adresse plus precise contentez-vous du Nom de La Ville 
                      suivit du Pays ex:Lomé-Togo.
                    </span>
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Mot de Passe
                </label>
                <div class="col-lg-9 col-xl-6">
                    {{ form_widget(form.MotDePasse,{'attr':{'class':'form-control form-control-lg form-control-solid'}}) }}
                    <span class="form-text text-muted">
                      Votre Mot de Passe a été crypté,vous pouvez le Modifier ici.Vous pouvez aussi le récuperer ou 
                      le modifier dans les Parametres de Mot de Passe.
                    </span>
                </div>
              </div>
              <div class="row">
                <label class="col-xl-3"></label>
                <div class="col-lg-9 col-xl-6">
                  <h5 class="font-weight-bold mt-10 mb-6">
                    Informations de Contact
                  </h5>
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Numero de Tel
                </label>
                <div class="col-lg-9 col-xl-6">
                  <div class="input-group input-group-lg input-group-solid">
                    <div class="input-group-prepend">
                      <span class="input-group-text">
                        <i class="la la-phone"></i>
                      </span>
                    </div>
                   {{ form_widget(form.Telephone,{'attr':{'class':'form-control form-control-lg form-control-solid'}}) }}
                  </div>
                </div>
              </div>
              <div class="form-group row">
                <label class="col-xl-3 col-lg-3 col-form-label">
                  Address Mail
                </label>
                <div class="col-lg-9 col-xl-6">
                  <div class="input-group input-group-lg input-group-solid">
                    <div class="input-group-prepend">
                      <span class="input-group-text">
                        <i class="la la-at"></i>
                      </span>
                    </div>
                     {{ form_widget(form.Email,{'attr':{'class':'form-control form-control-lg form-control-solid'}}) }}
                  </div>
                  <span class="form-text text-muted">
                    Votre Mail est Protége par la politique de confidentialité.
                  </span>
                </div>
              </div>
            </div>
            <!-- end::Body -->
          {{ form_end(form) }}
          <!-- end::Form -->
        </div>
      </div>
      <!-- end::Content -->
    </div>
    <!-- end::Profile Personal Information -->
  </div>
{% endblock %}

