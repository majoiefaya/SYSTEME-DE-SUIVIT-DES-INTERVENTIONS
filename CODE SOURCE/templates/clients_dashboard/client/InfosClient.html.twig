    <div class="container d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
      <!-- begin::Info -->
      <div class="d-flex align-items-center flex-wrap mr-1">
        <!-- begin::Mobile Toggle -->
        <button class="burger-icon burger-icon-left mr-4 d-inline-block d-lg-none"
          id="kt_subheader_mobile_toggle">
          <span></span>
        </button>
        <!-- end::Mobile Toggle -->

        <!-- begin::Page Heading -->
        <div class="d-flex align-items-baseline flex-wrap mr-5">
          <!-- begin::Page Title -->
          <h2 class="subheader-title text-dark font-weight-bold my-1 mr-3">
            Profile
          </h2>
          <!-- end::Page Title -->

          <!-- begin::Breadcrumb -->
          <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold my-2 p-0">
            <li class="breadcrumb-item">
              <a href="" class="text-muted">SDI</a>
            </li>
            <li class="breadcrumb-item">
              <a href="" class="text-muted">Utilisateur</a>
            </li>
            <li class="breadcrumb-item">
              <a href="" class="text-muted">Profile</a>
            </li>
            <li class="breadcrumb-item">
              <a href="" class="text-muted">Informations Personnelles</a>
            </li>
          </ul>
          <!-- end::Breadcrumb -->
        </div>
        <!-- end::Page Heading -->
      </div>
      <!-- end::Info -->
      {% if msg is defined %}
        <div class="alert alert-custom alert-success fade show py-4" role="alert">
            <div class="alert-icon"><i class="flaticon-warning"></i></div>
            <div class="alert-text font-weight-bold">
              {{ msg }}
            </div>
            <div class="alert-close">
                <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true"><i class="la la-close "></i></span>
                </button>
