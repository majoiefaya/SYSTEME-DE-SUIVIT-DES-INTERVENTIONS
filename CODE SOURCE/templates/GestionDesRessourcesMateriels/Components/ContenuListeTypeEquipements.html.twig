<div class="card card-custom gutter-b">
  <!-- begin::List Widget 19 -->
  <div class="card card-custom card-stretch gutter-b">
    <!-- begin::Header -->
    <div class="card-header border-0 pt-6 mb-2">
      <h3 class="card-title align-items-start flex-column">
        <span class="card-label font-weight-bold font-size-h4 text-dark-75 mb-3">
          Informations Des Differentes Categories
        </span>
        <span class="text-muted font-weight-bold font-size-sm">
          Vous avez Enregistrer Les {{ type_equipements|length }}
          Categories Suivantes
        </span>
      </h3>
      <div class="card-toolbar">
        <a href="{{ path('AjouterTypeEquipement') }}"
          class="btn btn-light-info btn-sm font-weight-bolder font-size-sm py-3 px-6">
          <span class="svg-icon svg-icon-primary svg-icon-2x">
            <!-- begin::Svg Icon | path:C:\wamp64\www\keenthemes\themes\metronic\theme\html\demo6\dist/../src/media/svg/icons\Code\Plus.svg -->
            <svg xmlns="http://www.w3.org/2000/svg"
              xmlns:xlink="http://www.w3.org/1999/xlink"
              width="24px"
              height="24px"
              viewBox="0 0 24 24"
              version="1.1">
              <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                <rect x="0" y="0" width="24" height="24" />
                <circle fill="#000000" opacity="0.3" cx="12" cy="12" r="10" />
                <path d="M11,11 L11,7 C11,6.44771525 11.4477153,6 12,6 C12.5522847,6 13,6.44771525 13,7 L13,11 L17,11 C17.5522847,11 18,11.4477153 18,12 C18,12.5522847 17.5522847,13 17,13 L13,13 L13,17 C13,17.5522847 12.5522847,18 12,18 C11.4477153,18 11,17.5522847 11,17 L11,13 L7,13 C6.44771525,13 6,12.5522847 6,12 C6,11.4477153 6.44771525,11 7,11 L11,11 Z"
                  fill="#000000" />
              </g>
            </svg>
            <!-- end::Svg Icon -->
          </span>
        </a>
      </div>
    </div>
    <!-- end::Header -->

    <!-- begin::Body -->
    <div class="card-body pt-2">
      <!-- begin::Table -->
      <div class="table-responsive">
        <table class="table table-borderless mb-0">
          <thead>
            <tr class="text-uppercase">
              <th style="min-width: 250px" class="pl-7"></th>
              <th style="min-width: 120px">
                Libelle
              </th>
              <th style="min-width: 120px">
                Description
              </th>
              <th style="min-width: 120px">
                Quantite
              </th>
              <th style="min-width: 120px">
                Consulter
              </th>
            </tr>
          </thead>
          <tbody>
            <!-- begin::Item -->
            {% for typeEquipement in type_equipements %}
              <tr>
                <td scope="row" class="w-40px align-middle pb-6 pl-0 pr-2">
                  <!-- begin::Symbol -->
                  <div class="symbol symbol-40 symbol-light-warning align-middle">
                    <span class="symbol-label">
                      <span class="svg-icon svg-icon-lg svg-icon-warning">
                        <!-- begin::Svg Icon | path:assets/media/svg/icons/Home/Library.svg -->
                        <svg xmlns="http://www.w3.org/2000/svg"
                          xmlns:xlink="http://www.w3.org/1999/xlink"
                          width="24px"
                          height="24px"
                          viewBox="0 0 24 24"
                          version="1.1">
                          <g stroke="none"
                            stroke-width="1"
                            fill="none"
                            fill-rule="evenodd">
                            <rect x="0" y="0" width="24" height="24"></rect>
                            <path d="M5,3 L6,3 C6.55228475,3 7,3.44771525 7,4 L7,20 C7,20.5522847 6.55228475,21 6,21 L5,21 C4.44771525,21 4,20.5522847 4,20 L4,4 C4,3.44771525 4.44771525,3 5,3 Z M10,3 L11,3 C11.5522847,3 12,3.44771525 12,4 L12,20 C12,20.5522847 11.5522847,21 11,21 L10,21 C9.44771525,21 9,20.5522847 9,20 L9,4 C9,3.44771525 9.44771525,3 10,3 Z"
                              fill="#000000"></path>
                            <rect fill="#000000"
                              opacity="0.3"
                              transform="translate(17.825568, 11.945519) rotate(-19.000000) translate(-17.825568, -11.945519) "
                              x="16.3255682"
                              y="2.94551858"
                              width="3"
                              height="18"
                              rx="1"></rect>
                          </g>
                        </svg>
                        <!-- end::Svg Icon -->
                      </span>
                    </span>
                  </div>
                  <!-- end::Symbol -->
                </td>
                <td>
                  {{ typeEquipement.Libelle }}
                </td>
                <td>
                  {{ typeEquipement.Description }}
                </td>
                <td>
                  {{ typeEquipement.equipement|length }}
                </td>
                <td>
                  <a href="{{
                    path(
                      'InfoTypeEquipement',
                      {
                        id: typeEquipement.id
                      }
                    )
                    }}"
                    class="btn btn-icon btn-light btn-hover-primary btn-sm mx-3">
                    <span class="svg-icon svg-icon-primary svg-icon-2x">
                      <!-- begin::Svg Icon | path:C:\wamp64\www\keenthemes\themes\metronic\theme\html\demo6\dist/../src/media/svg/icons\Communication\Forward.svg -->
                      <svg xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink"
                        width="24px"
                        height="24px"
                        viewBox="0 0 24 24"
                        version="1.1">
                        <g stroke="none"
                          stroke-width="1"
                          fill="none"
                          fill-rule="evenodd">
                          <rect x="0" y="0" width="24" height="24" />
                          <path d="M12.6571817,10 L12.6571817,5.67013288 C12.6571817,5.25591932 12.3213953,4.92013288 11.9071817,4.92013288 C11.7234961,4.92013288 11.5461972,4.98754181 11.4089088,5.10957589 L4.25168161,11.4715556 C3.94209454,11.7467441 3.91420899,12.2207984 4.1893975,12.5303855 C4.19915701,12.541365 4.209237,12.5520553 4.21962441,12.5624427 L11.3768516,19.7196699 C11.6697448,20.0125631 12.1446186,20.0125631 12.4375118,19.7196699 C12.5781641,19.5790176 12.6571817,19.3882522 12.6571817,19.1893398 L12.6571817,15 C14.004369,14.9188289 16.83481,14.9157978 21.1485046,14.9909069 L21.1485051,14.9908794 C21.4245904,14.9956866 21.6522988,14.7757721 21.6571059,14.4996868 C21.6571564,14.4967857 21.6571817,14.4938842 21.6571817,14.4909827 L21.6572352,10.5050185 C21.6572352,10.2288465 21.4333536,10.0049649 21.1571817,10.0049649 C21.1555649,10.0049649 21.1539481,10.0049728 21.1523314,10.0049884 C16.0215539,10.0547574 13.1898373,10.0530946 12.6571817,10 Z"
