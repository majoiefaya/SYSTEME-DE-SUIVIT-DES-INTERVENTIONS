              <div class="d-flex align-items-center mr-10">
                <div class="mr-6">
                  <div class="font-weight-bold mb-2">
                    Date Debut
                  </div>
                  <span class="btn btn-sm btn-text btn-light-danger text-uppercase font-weight-bold">
                    {% if intervention.DateDebutIntervention != null %}
                      {{ intervention.DateDebutIntervention|date('d-m-Y') }}
                    {% else %}
                      N/A
                    {% endif %}
                  </span>
                </div>
                <div class="">
                  <div class="font-weight-bold mb-2">
                    Date Fin
                  </div>
                  <span class="btn btn-sm btn-text btn-light-danger text-uppercase font-weight-bold">
                    {% if intervention.DateFinIntervention != null %}
                      {{ intervention.DateFinIntervention|date('d-m-Y') }}
                    {% else %}
                      N/A
                    {% endif %}
                  </span>
                </div>
              </div>
              <div class="flex-grow-1 flex-shrink-0 w-150px w-xl-300px mt-4 mt-sm-0">
                <span class="font-weight-bold">Progression</span>
                <div class="progress progress-xs mt-2 mb-2">
                  <div class="progress-bar bg-success"
