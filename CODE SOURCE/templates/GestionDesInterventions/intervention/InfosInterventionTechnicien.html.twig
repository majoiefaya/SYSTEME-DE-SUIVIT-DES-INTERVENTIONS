                  </span> Localiser Intervention
                </a>
              </div>
              <!-- end::Contacts -->
            </div>
            <div class="my-lg-0 my-1">
            {% if intervention.interventionAss is empty %}
              <a href="#"
                class="btn btn-sm btn-info font-weight-bolder text-uppercase">
                Non Assistée
              </a>
            {% else %}
              <a href="#"
                class="btn btn-sm btn-light-success font-weight-bolder text-uppercase mr-3">
                Assistée
              </a>
            {% endif %}
            {% if intervention.DateDebutIntervention==null or intervention.DateFinIntervention==null or intervention.client==null or intervention.equipes is empty %}
              <a href="{{path('ActiverIntervention',{'id':intervention.id})}}"
                class="btn btn-sm btn-info font-weight-bolder text-uppercase">
                Activer
              </a>
            {% else %}
              <a href="#"
                class="btn btn-sm btn-light-success font-weight-bolder text-uppercase mr-3">
                Desactiver
              </a>
            {% endif %}
            </div>
          </div>
